{
  "name": "lovelace-pronote",
  "version": "1.0.1",
  "description": "Lovelace cards for Pronote",
  "main": "dist/pronote.js",
  "scripts": {
    "build": "npm run lint && npm run rollup",
    "rollup": "rollup -c",
    "babel": "babel dist/pronote.js --out-file dist/pronote.js",
    "lint": "eslint src/*.{ts,js}",
    "watch": "rollup -c --watch",
    "postversion": "npm run build",
    "audit-fix": "npx yarn-audit-fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delphiki/lovelace-pronote.git"
  },
  "keywords": [
    "lovelace"
  ],
  "author": "delphiki",
  "contributors": [
    "delphiki (https://github.com/delphiki)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delphiki/lovelace-pronote/issues"
  },
  "homepage": "https://github.com/delphiki/lovelace-pronote#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.5",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "7.12.1",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jsdom": "^28.0.0",
    "npm": "^11.9.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "rollup": "^2.79.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "semantic-release": "^19.0.3",
    "ts-lit-plugin": "^1.1.10",
    "typescript": "^4.0.5",
    "typescript-styled-plugin": "^0.15.0",
    "vitest": "^4.0.18",
    "yarn-audit-fix": "^9.3.10"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.2.0",
    "@material/mwc-ripple": "^0.19.1",
    "fast-copy": "^2.1.0",
    "home-assistant-js-websocket": "^8.2.0",
    "lit": "^3.3.2",
    "lit-element": "^3.3.2",
    "lit-html": "^2.7.5",
    "memoize-one": "^6.0.0"
  }
}
